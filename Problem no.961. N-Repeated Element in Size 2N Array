PROBLEM     NUMBER          :                 961. N-Repeated Element in Size 2N Array

// Friday, 2 January 2026

//  Pigeonhole Principle | Sliding Window | Greedy

class Solution {
public:
    int repeatedNTimes(vector<int>& A) {
        for (int i = 0; i < A.size() - 2; ++i)
            if (A[i] == A[i + 1] || A[i] == A[i + 2])
                return A[i];
        return A.back();
    }
};
