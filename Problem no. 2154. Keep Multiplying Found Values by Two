PROBLEM    NUMBER                                    :                                      2154. Keep Multiplying Found Values by Two


//  Wednesday, 19 November 2025



//  Bit Manipulation

class Solution {
public:
    int findFinalValue(vector<int>& nums, int original) {
        int cnt[1001] = {0};
        for (int &x : nums) {
            cnt[x]++;
        }        
        while (original <= 1000 && cnt[original] > 0) {
            original *= 2;
        }
        return original;
    }
};
